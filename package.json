{
  "name": "nextjs-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint --fix --max-warnings 0",
    "test": "vitest run",
    "test:w": "vitest --watch",
    "coverage": "vitest run --coverage",
    "type:check": "tsc",
    "format": "prettier --log-level warn --write \"./**/*.{ts,tsx,d.ts,mjs,json,css,scss,yml,md}\"",
    "docker": "docker compose up --build -d",
    "docker-stage": "docker compose -f docker-compose-stage.yml up --build -d",
    "docker:sh": "docker exec -it frontend sh",
    "docker-stage:sh": "docker exec -it frontend_stage sh",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.0.1",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-icons": "^5.3.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@next/eslint-plugin-next": "^15.0.1",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "pnpm": ">=9"
  }
}
